<% conversation.participants.each do |participant| %>
  <% unless participant == current_user %>
   <%if participant.teenager%>
    <strong><%=Teenager.find_by(user_id: participant.id).fname%> </strong>
    <%else%>
    <strong><%=Client.find_by(user_id: participant.id).fname%> </strong>
    <%end%>
       <% end %>
<% end %>